// Place your key bindings in this file to overwrite the defaults
[
    { "key": "cmd+shift+]",            "command": "workbench.files.action.openNextWorkingFile" },
    { "key": "cmd+shift+[",            "command": "workbench.files.action.openPreviousWorkingFile" },
    { "key": "cmd+w",                  "command": "workbench.files.action.closeFile" },
    { "key": "ctrl+w",                  "command": "workbench.action.closeActiveEditor" },
    { "key": "ctrl+left",              "command": "subwordNavigation.cursorSubwordLeft" },
    { "key": "ctrl+right",             "command": "subwordNavigation.cursorSubwordRight" },
    { "key": "ctrl+shift+left",        "command": "subwordNavigation.cursorSubwordLeftSelect" },
    { "key": "ctrl+shift+right",       "command": "subwordNavigation.cursorSubwordRightSelect" }
]